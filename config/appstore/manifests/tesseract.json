{
  "id": "tesseract",
  "name": "Tesseract OCR",
  "description": "Open-Source Texterkennung fuer Dokumente und Bilder. Unterstuetzt 100+ Sprachen.",
  "version": "5.3.0",
  "category": "ai",
  "appType": "official",
  "icon": "FiType",
  "image": "clearlinux/tesseract-ocr:latest",
  "ports": {
    "internal": 8080,
    "external": 8085
  },
  "volumes": [
    {
      "host": "/arasul/data/tesseract",
      "container": "/data"
    }
  ],
  "environment": {
    "TESSDATA_PREFIX": "/usr/share/tessdata",
    "OMP_THREAD_LIMIT": "4"
  },
  "resources": {
    "memory": "1g",
    "cpus": "2"
  },
  "healthcheck": {
    "test": ["CMD", "tesseract", "--version"],
    "interval": "30s",
    "timeout": "10s",
    "retries": 3
  },
  "traefik": {
    "enabled": false
  },
  "dependencies": [],
  "capabilities": ["ocr", "pdf", "multi-language", "document-scanning"],
  "recommended_for": ["document-processing", "pdf-text-extraction", "image-text"],
  "featured": false
}
